    <script type="text/javascript">

        var pageSize = 6;
        var pageIndex = 0;

        $(document).ready(function () {
            GetData();

            $(window).scroll(function () {
                if ($(window).scrollTop() ==
                    $(document).height() - $(window).height()) {
                    if (@count !== $(".post-item").length) {
                        GetData();
                    }

                }
            });
        });

        function GetData() {
            $.ajax({
                type: 'GET',
                url: '/Home/GetData',
                data: { "pageindex": pageIndex, "pagesize": pageSize },
                dataType: 'json',
                beforeSend: function () {
                    $("#main-wrapper").append(`<div class="spinner-border d-flex mx-auto" id="progress" role="status">
            <span class="sr-only">Loading...</span>
        </div>`);
                },
                success: function (data) {
                    var element = "";
                    console.log(data)
                    if (data != null) {
                        for (var i = 0; i < data.length; i++) {
                            //var d = new Date();
                            //var curr_date = d.getDate(data[i].time) + "/" + d.getMonth(data[i].time + "1") + "/" + d.getFullYear(data[i].time);

                            element += `<div class="col-md-6 post-item">
                                                        <!--Single Blog Post Start-->
                                                        <div class="single-blog-post mb-30">
                                                            <div class="blog-img">
                                                                <a href="Home/ArticleContent/${data[i].id}"><img src="/img/${data[i].coverPhotoPath}" width="555" height="313" alt=""></a>
                                                            </div>
                                                            <div class="blog-content">
                                                                <h3><a href="single-blog.html" style="color:#CC0066;">${data[i].title}</a></h3>
                                                                <p class="boranAga" style="">${data[i].content}</p>
                                                                <ul class="meta">
                                                                    <li><a href="#">Admin</a></li>

                                                                        <li>${new Date(data[i].time).toLocaleDateString()}</li>
                                                                    <li><a href="#">25 Comments</a></li>
                                                                </ul>
                                                                </div>
                                                            </div>
                                                            <!--Single Blog Post End-->
                                                        </div>`;

                        }

                            setTimeout(function () {
                                $("#scrollPage").append(element)
                            }, 750)
                            pageIndex++;
                    }
                },
                complete: function () {
                    $("#progress").remove()
                },
                error: function () {
                    alert("Error while retrieving data!");
                }
            });
        }






    </script>
    <script>
        $(window).scroll(function () {
            var navbar = document.getElementById("rightBar");
            if (window.pageYOffset >= 696) {
                navbar.classList.add("sticky")
            } else {
                navbar.classList.remove("sticky");
            }
        })
    </script>

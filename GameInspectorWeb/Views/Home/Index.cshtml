    <script type="text/javascript">

        var pageSize = 6;
        var pageIndex = 0;

        $(document).ready(function () {
            GetData();

            $(window).scroll(function () {
                if ($(window).scrollTop() ==
                    $(document).height() - $(window).height()) {
                    if (@count !== $(".post-item").length) {
                        GetData();
                    }

                }
            });
        });

        function GetData() {
            $.ajax({
                type: 'GET',
                url: '/Home/GetData',
                data: { "pageindex": pageIndex, "pagesize": pageSize },
                dataType: 'json',
                success: function (data) {
                    var element = "";
                    console.log(data)
                    if (data != null) {
                        for (var i = 0; i < data.length; i++) {
                             element += `<div class="col-md-6 post-item">
                                                        <!--Single Blog Post Start-->
                                                        <div class="single-blog-post mb-30">
                                                            <div class="blog-img">
                                                                <a href="single-blog.html"><img src="/img/${data[i].coverPhotoPath}" width="555" height="313" alt=""></a>
                                                            </div>
                                                            <div class="blog-content">
                                                                <h3><a href="single-blog.html">${data[i].title}</a></h3>
                                                                <p rows="3">${data[i].content}</p>
                                                                <ul class="meta">
                                                                    <li><a href="#">Admin</a></li>
                                                                    <li>${data[i].time}</li>
                                                                    <li><a href="#">25 Comments</a></li>
                                                                </ul>
                                                                </div>
                                                            </div>
                                                            <!--Single Blog Post End-->
                                                        </div>`;

                        }

                            setTimeout(function () {
                                $("#scrollPage").append(element)
                            }, 750)
                            pageIndex++;
                    }
                },
                beforeSend: function () {
                    $("#progress").show();
                },
                complete: function () {
                    $("#progress").hide();
                },
                error: function () {
                    alert("Error while retrieving data!");
                }
            });
        }
    </script>
